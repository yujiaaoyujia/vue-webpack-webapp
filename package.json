{
  "name": "vue-webpack-webapp",
  "version": "1.0.0",
  "description": "Webpack 4.0 Vuejs Webapp template",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config build/webpack.config.dev.js",
    "dev:mock": "cross-env NODE_ENV=development MOCK=true webpack-dev-server --progress --config build/webpack.config.dev.js",
    "dist": "cross-env NODE_ENV=production webpack --config build/webpack.config.prod.js",
    "build": "node build/build.js",
    "build:mock": "cross-env MOCK=true node build/build.js",
    "gzip": "node build/gzip.js",
    "server": "node build/server.js",
    "mocha": "mochapack --webpack-config build/webpack.config.test.js --require tests/mocha/setup.js --glob *.test.js tests/mocha --recursive --watch",
    "mocha:nyc": "nyc --reporter=lcov --reporter=text mochapack --webpack-config build/webpack.config.test.js --require tests/mocha/setup.js --glob *.test.js tests/mocha --recursive",
    "cypress": "cypress open",
    "test:unit": "npm run mocha",
    "test:e2e": "npm run cypress"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "author": "zero",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.5.5",
    "@tweenjs/tween.js": "^17.4.0",
    "axios": "^0.19.0",
    "core-js": "^3.2.1",
    "dayjs": "^1.8.15",
    "number-precision": "^1.3.1",
    "qs": "^6.7.0",
    "regenerator-runtime": "^0.13.3",
    "vant": "^2.10.6",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "compressing": "^1.5.1",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "cypress": "^3.7.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "expect": "^24.9.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.1.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.2",
    "mochapack": "^1.1.13",
    "mockjs": "^1.0.1-beta3",
    "node-notifier": "^5.4.1",
    "nyc": "^11.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^3.4.0",
    "portfinder": "^1.0.21",
    "postcss-html": "^0.36.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-write-svg": "^3.0.1",
    "rimraf": "^2.6.3",
    "script-loader": "^0.7.2",
    "serve-handler": "^6.1.0",
    "style-loader": "^1.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-plugin-stylus": "^0.9.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^4.1.0",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "vue-server-renderer": "^2.6.10",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "nyc": {
    "include": [
      "tests/mocha"
    ],
    "exclude": [
      "tests/mocha/setup.js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
