{
  "presets": [
    ["env", {
      "modules": false
    }]
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["transform-object-rest-spread"],
    ["syntax-dynamic-import"],
    ["component", [{
      "libraryName": "mint-ui",
      "style": true
    }]]
  ]
}
